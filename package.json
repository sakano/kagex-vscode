{
    "name": "kagex-vscode",
    "displayName": "KAG/KAGEX",
    "description": "Adding KAG/KAGEX language support for Visual Studio Code",
    "version": "0.1.0",
    "publisher": "Biscrat",
    "license": "MIT",
    "icon": "images/kagicon.png",
    "engines": {
        "vscode": "^1.21.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/sakano/kagex-vscode.git"
    },
    "bugs": {
        "url": "https://github.com/sakano/kagex-vscode/issues"
    },
    "categories": [
        "Languages",
        "Snippets"
    ],
    "keywords": [
        "kirikiri"
    ],
    "contributes": {
        "languages": [
            {
                "id": "kagex",
                "aliases": [
                    "kagex",
                    "kag"
                ],
                "extensions": [
                    ".ks"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "kagex",
                "scopeName": "source.kagex",
                "path": "./syntaxes/kagex.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "kagex",
                "path": "./snippets/kagex.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "main": "./out/src/extension",
    "activationEvents": [
    ]
}
